<header>
            <nav class="navbar  navbar-expand-lg">
                <div class="container-fluid">
                    <a class="navbar-brand" href="./">
                        <img src="../images/logo-no-background.png" alt="logo" width="55" height="45">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon lines"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/dashboard">Dashboard</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/profile/{{user.id}}">Profile</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/">Log Out</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
</header>

{{#if rsvps.length}}
<section>
  <h1><a class="link" href="/single-event/{{event.id}}">{{event_title}}'s Attendees</a></h1>
    {{#each rsvps as |rsvp|}}
      {{> rsvp-info rsvp}}
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/rsvp">+ New RSVP</a></button>

<script src="../js/rsvp.js"></script>
<script src="/js/logout.js"></script>